datasource db {
  provider = "sqlite"
  url = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Room {
  id     Int @id @default(autoincrement())
  user1  String
  user2  String
  name   String @unique // Random slug generated at app level
  isOpen Boolean @default(false)
  attemptId String?
  createdAt DateTime @default(now())
}